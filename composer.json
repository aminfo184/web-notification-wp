 {
     "name": "web-notification-wp/plugin",
     "description": "Web Push Notification Plugin for WordPress",
     "type": "wordpress-plugin",
     "require": {
         "php": ">=8.2",
         "minishlink/web-push": "^9.0"
     },
     "require-dev": {
         "phpunit/phpunit": "^9.0"
     },
     "autoload": {
         "psr-4": {
             "WebNotificationWP\\": "includes/classes/"
         }
     },
     "config": {
         "optimize-autoloader": true,
         "prefer-stable": true,
         "sort-packages": true
     },
     "minimum-stability": "stable"
 }